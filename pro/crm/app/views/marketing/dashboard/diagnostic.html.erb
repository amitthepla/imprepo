<div class="col-md-6">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading" style="border-bottom: solid red 1px;">
        <span class="h4">Lead Source</span>
        <ul class="tool-bar">
          <li><a href="#collapseWidget" data-toggle="collapse"><i class="fa fa-arrows-v"></i></a></li>
        </ul>
      </div>
      <div class="padding-md" style="border-bottom: solid red 1px;">
        This table displays the historic and comparitive data for each source type. Click the source type to view the
        many sources that belong to your organization.
      </div>
      <div id="collapseWidget" class="padding-md panel-body no-padding collapse text-primary" >
        <table class="table table-hover table-striped text-center table-bordered">
          <thead>
            <tr>
              <th class="text-center"><h5>Source</h5></th>
              <th class="text-center"><h5>First Data Set</h5></th>
              <th class="text-center"><h5>Second Data Set</h5></th>
              <th class="text-center"><h5>Difference</h5></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>Internet</td>
              <td><a data-target="#first_internet_sources" role="button" data-toggle="modal"><%= source_count_check(@first_sources["Internet"]) -%></a></td>
              <td><a data-target="#second_internet_sources" role="button" data-toggle="modal"><%= source_count_check(@second_sources["Internet"])  -%></a></td>
              <% internet = source_count_check(@second_sources["Internet"]) - source_count_check(@first_sources["Internet"]) %>
              <td>
                <span class="<%= positive_or_negative?(internet) %>">
                  <%= internet -%>
                </span>
              </td>
            </tr>
            <tr>
              <td>Referral</td>
              <td><a data-target="#first_referral_sources" role="button" data-toggle="modal"><%= source_count_check(@first_sources["Referral"]) -%></a></td>
              <td><a data-target="#second_referral_sources" role="button" data-toggle="modal"><%= source_count_check(@second_sources["Referral"]) -%></a></td>
              <% referral = source_count_check(@second_sources["Referral"]) - source_count_check(@first_sources["Referral"])  %>
              <td>
                <span class="<%= positive_or_negative?(referral) %>">
                  <%= referral -%>
                </span>
              </td>
            </tr>
            <tr>
              <td>Social Media</td>
              <td><a data-target="#first_social_media_sources" role="button" data-toggle="modal"><%= source_count_check(@first_sources["Social Media"]) -%></a></td>
              <td><a data-target="#second_social_media_sources" role="button" data-toggle="modal"><%= source_count_check(@second_sources["Social Media"]) -%></a></td>
              <% relations = source_count_check(@second_sources["Social Media"]) - source_count_check(@first_sources["Social Media"]) %>
              <td>
                <span class="<%= positive_or_negative?(relations)%>">
                  <%= relations -%>
                </span>
              </td>
            </tr>
            <tr>
              <td>Traditional Media</td>
              <td><a data-target="#first_traditional_media_sources" role="button" data-toggle="modal"><%= source_count_check(@first_sources["Traditional Media"]) -%></a></td>
              <td><a data-target="#second_traditional_media_sources" role="button" data-toggle="modal"><%= source_count_check(@second_sources["Traditional Media"]) -%></a></td>
              <% radio = source_count_check(@second_sources["Traditional Media"]) - source_count_check(@first_sources["Traditional Media"]) %>
              <td>
                <span class="<%= positive_or_negative?(radio) %>">
                  <%= radio -%>
                </span>
              </td>
            </tr>
            <tr>
              <td>Events</td>
              <td><a data-target="#first_events_sources" role="button" data-toggle="modal"><%= source_count_check(@first_sources["Events"])  -%></a></td>
              <td><a data-target="#second_events_media_sources" role="button" data-toggle="modal"><%= source_count_check(@second_sources["Events"])  -%></a></td>
              <% prints = source_count_check(@second_sources["Events"]) - source_count_check(@first_sources["Events"]) %>
              <td>
                <span class="<%= positive_or_negative?(prints) %>">
                  <%= prints -%>
                </span>
              </td>
            </tr>
            <tr>
              <td>Telemarketing</td>
              <td><a data-target="#first_telemarketing_sources" role="button" data-toggle="modal"><%= source_count_check(@first_sources["Telemarketing"]) -%></a></td>
              <td><a data-target="#second_telemarketing_sources" role="button" data-toggle="modal"><%= source_count_check(@second_sources["Telemarketing"]) -%></a></td>
              <% events = source_count_check(@second_sources["Telemarketing"]) - source_count_check(@first_sources["Telemarketing"])  %>
              <td>
                <span class="<%= positive_or_negative?(events) %>">
                  <%= events -%>
                </span>
              </td>
            </tr>
            <tr>
              <td>Walk-In</td>
              <td><a data-target="#first_walk_in_sources" role="button" data-toggle="modal"><%= source_count_check(@first_sources["Walk-In"]) -%></a></td>
              <td><a data-target="#second_walk_in_sources" role="button" data-toggle="modal"><%= source_count_check(@second_sources["Walk-In"]) -%></a></td>
              <% tv = source_count_check(@second_sources["Walk-In"]) - source_count_check(@first_sources["Walk-In"]) %>
              <td>
                <span class="<%= positive_or_negative?(tv) %>">
                  <%= tv -%>
                </span>
              </td>
            </tr>
            <tr>
              <td>Other</td>
              <td><a data-target="#first_other_sources" role="button" data-toggle="modal"><%= source_count_check(@first_sources["Other"]) -%></a></td>
              <td><a data-target="#second_other_sources" role="button" data-toggle="modal"><%= source_count_check(@second_sources["Other"]) -%></a></td>
              <% outdoor = source_count_check(@second_sources["Other"]) - source_count_check(@first_sources["Other"]) %>
              <td>
                <span class="<%= positive_or_negative?(outdoor) %>">
                  <%= outdoor -%>
                </span>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </div><!-- /panel -->
  </div><!-- /.col -->

  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading" style="border-bottom: solid red 1px;">
        <span class="h4">Lead Generation</span>
        <ul class="tool-bar">
          <li><a href="#collapseLeadGen" data-toggle="collapse"><i class="fa fa-arrows-v"></i></a></li>
        </ul>
      </div>
      <div class="padding-md" style="border-bottom: solid red 1px;">
        You can see leads who inquire Electronically and leads who inquire by Telephone or Walk In.
      </div>
      <div class="panel-body no-padding collapse" id="collapseLeadGen">
        <table class="table table-hover table-striped text-center table-bordered">
          <thead>
            <tr>
              <th class="text-center"><h5>Inquiry Type</h5></th>
              <th class="text-center"><h5>First Data Set</h5></th>
              <th class="text-center"><h5>Second Data Set</h5></th>
              <th class="text-center"><h5>Difference</h5></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>Web Inquiry</td>
              <td><%= @first_web_leads_count -%></td>
              <td><%= @second_web_leads_count -%></td>
              <% electronic =  @second_web_leads_count - @first_web_leads_count  %>
              <td>
                <span class="<%= positive_or_negative?(electronic)%>">
                  <%= electronic %>
                </span>
              </td>
            </tr>
            <tr>
              <td>Phone Call/ Walk In</td>
              <td><%= @first_phone_leads_count -%></td>
              <td><%= @second_phone_leads_count -%></td>
              <% phone_call = @second_phone_leads_count - @first_phone_leads_count  %>
              <td>
                <span class="<%= positive_or_negative?(phone_call)%>">
                  <%= phone_call %>
                </span>
              </td>
            </tr>
            <tr>
              <td>Total</td>
              <td><%= @first_leads_count -%></td>
              <td><%= @second_leads_count -%></td>
              <% total_leads = @second_leads_count - @first_leads_count %>
              <td>
                <span class="<%= positive_or_negative?(total_leads)%>">
                  <%= total_leads %>
                </span>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </div><!-- /panel -->
  </div><!-- /.col -->

  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading" style="border-bottom: solid red 1px;">
        <span class="h4">Surgery Completion</span>
        <ul class="tool-bar">
          <li><a href="#collapseSurgery" data-toggle="collapse"><i class="fa fa-arrows-v"></i></a></li>
        </ul>
      </div>
      <div class="padding-md" style="border-bottom: solid red 1px;">
        You can see leads who scheduled, completed and cancelled surgery.
      </div>
      <div class="panel-body no-padding collapse" id="collapseSurgery">
        <table class="table table-hover table-striped text-center table-bordered">
          <thead>
            <tr>
              <th class="text-center"><h5>Surgery Status</h5></th>
              <th class="text-center"><h5>First Data Set</h5></th>
              <th class="text-center"><h5>Second Data Set</h5></th>
              <th class="text-center"><h5>Difference</h5></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>Scheduled</td>
              <td><%= @first_surgery_count -%></td>
              <td><%= @second_surgery_count -%></td>
              <% scheduled = @second_surgery_count - @first_surgery_count  %>
              <td class="<%= positive_or_negative?(scheduled) %>">
                <%= scheduled %>
              </td>
            </tr>
            <tr>
              <td>Completed</td>
              <td><%= @first_completed_surgery_count -%></td>
              <td><%= @second_completed_surgery_count -%></td>
              <% completed = @second_completed_surgery_count - @first_completed_surgery_count %>
              <td class="<%= positive_or_negative?(completed) %>">
                <%= completed %>
              </td>
            </tr>
            <tr>
              <td>Cancelled</td>
              <td><%= @first_cancelled_surgery_count -%></td>
              <td><%= @second_cancelled_surgery_count -%></td>
              <% cancelled = @second_cancelled_surgery_count - @first_cancelled_surgery_count  %>
              <td class="<%= positive_or_negative?(cancelled) %>">
                <%= cancelled %>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </div><!-- /panel -->
  </div>
</div>

<div class="col-md-6">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading" style="border-bottom: solid red 1px;">
        <span class="h4">Inquiry to Appointment</span>
        <ul class="tool-bar">
          <li><a href="#collapseITA" data-toggle="collapse"><i class="fa fa-arrows-v"></i></a></li>
        </ul>
      </div>
      <div class="padding-md" style="border-bottom: solid red 1px;">
        You can see the Inquiry to Appointment rate for the practice and all users.
      </div>
      <div class="panel-body no-padding collapse" id="collapseITA">
        <table class="table table-hover table-striped text-center table-bordered">
          <thead>
            <tr>
              <th style="width:25%" class="text-center"><h5>User</h5></th>
              <th style="width:25%" class="text-center"><h5>First Data Set</h5></th>
              <th style="width:25%" class="text-center"><h5>Second Data Set</h5></th>
              <th style="width:25%" class="text-center"><h5>Difference</h5></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td><%= @current_org.company_name %></td>
              <td> <%= @first_leads_count == 0 ? 0 : ((@first_consult_count.to_f / @first_leads_count) * 100).round(2) %>%</td>
              <td> <%= @second_leads_count == 0 ? 0 : ((@second_consult_count.to_f / @second_leads_count) * 100).round(2) %>%</td>
              <% total = (@second_leads_count == 0 ? 0 : ((@second_consult_count.to_f / @second_leads_count) * 100).round(2))  - (@first_leads_count == 0 ? 0 : ((@first_consult_count.to_f / @first_leads_count) * 100).round(2)) %>
              <td class="<%= positive_or_negative?(total) %>">
                <%= total.round(2) %>%
              </td>
            </tr>
            <tr>
              <td>Phone / Walk In</td>
              <td> <%= @first_phone_leads_count == 0 ? 0 : ((@first_phone_consult_count.to_f / @first_phone_leads_count) * 100).round(2)  %>%</td>
              <td> <%= @second_phone_leads_count == 0 ? 0 : ((@second_phone_consult_count.to_f / @second_phone_leads_count) * 100).round(2)  %>%</td>
              <% phone =  (@second_phone_leads_count == 0 ? 0 : ((@second_phone_consult_count.to_f / @second_phone_leads_count) * 100).round(2)) - (@first_phone_leads_count == 0 ? 0 : ((@first_phone_consult_count.to_f / @first_phone_leads_count) * 100).round(2)) %>
              <td class="<%= positive_or_negative?(phone) %>">
                <%= phone.round(2) %>%
              </td>
            </tr>
            <tr>
              <td>Web</td>
              <td> <%= @first_web_leads_count == 0 ? 0 : ((@first_web_consult_count.to_f / @first_web_leads_count) * 100).round(2)  %>%</td>
              <td> <%= @second_web_leads_count == 0 ? 0 : ((@second_web_consult_count.to_f / @second_web_leads_count) * 100).round(2) %>%</td>
              <% email = (@second_web_leads_count == 0 ? 0 : ((@second_web_consult_count.to_f / @second_web_leads_count) * 100).round(2)) - (@first_web_leads_count == 0 ? 0 : ((@first_web_consult_count.to_f / @first_web_leads_count) * 100).round(2)) %>
              <td class="<%= positive_or_negative?(email) %>">
                <%= email.round(2) %>%
              </td>
            </tr>
          </tbody>
        </table>
        <div class="panel-group" id="accordion">
          <div class="panel panel-default">
            <div class="panel-heading">
              <h4 class="panel-title">
                <a class="accordion-toggle text-primary" data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                  <center>
                    <span class="h5 text-primary"><strong>Inquiry to Appointment Rate for each Receptionist</strong></span>
                  </center>
                </a>
              </h4>
            </div>
            <div id="collapseOne" class="panel-collapse collapse">
              <div class="panel-body">
                <div id="ita-individual">
                  <table class="table table-hover table-striped text-center table-bordered">
                    <thead>
                      <tr>
                        <th style="width:25%" class="text-center">User</th>
                        <th style="width:25%" class="text-center">First Data Set</th>
                        <th style="width:25%" class="text-center">Second Data Set</th>
                        <th style="width:25%" class="text-center">Difference</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% @first_ita_users.each do |k, v| -%>
                      <tr>
                        <td><%= k.to_s.titleize %></td>
                        <td><span><%= v %>%</span></td>
                        <td><span><%= @second_ita_users[k] %>%</span></td>
                        <td>
                          <% ita_difference = @second_ita_users[k] - v  %>
                          <span class="<%= positive_or_negative?(ita_difference)%>">
                            <%= ita_difference.round(2) %>%
                          </span>
                        </td>
                      </tr>
                      <%- end -%>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div><!-- /panel -->
        </div>
      </div>
    </div><!-- /panel -->
  </div><!-- /.col -->

  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading" style="border-bottom: solid red 1px;">
        <span class="h4">Closing Rate</span>
        <ul class="tool-bar">
          <li><a href="#collapseClosing" data-toggle="collapse"><i class="fa fa-arrows-v"></i></a></li>
        </ul>
      </div>
      <div class="padding-md" style="border-bottom: solid red 1px;">
        You can see the closing rate for the practice and all users.
      </div>
      <div class="panel-body no-padding collapse" id="collapseClosing">
        <table class="table table-hover table-striped text-center table-bordered">
          <thead>
            <tr>
              <th style="width:25%" class="text-center">User</th>
              <th style="width:25%" class="text-center">First Data Set</th>
              <th style="width:25%" class="text-center">Second Data Set</th>
              <th style="width:25%" class="text-center">Difference</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td><%= @current_org.company_name %></td>
              <td> <%= @first_consult_count == 0 ? 0 : ((@first_surgery_count.to_f / @first_consult_count) * 100).round(2) %>%</td>
              <td> <%= @second_consult_count == 0 ? 0 : ((@second_surgery_count.to_f / @second_consult_count) * 100).round(2) %>%</td>
              <td class="<%=positive_or_negative?(((@second_consult_count == 0 ? 0 : ((@second_surgery_count.to_f / @second_consult_count) * 100).round(2)) - (@first_consult_count == 0 ? 0 : ((@first_surgery_count.to_f / @first_consult_count) * 100).round(2))).round(2) )%>">
                <%= ((@second_consult_count == 0 ? 0 : ((@second_surgery_count.to_f / @second_consult_count) * 100).round(2)) - (@first_consult_count == 0 ? 0 : ((@first_surgery_count.to_f / @first_consult_count) * 100).round(2))).round(2) %>%
              </td>
            </tr>
          </tbody>
        </table>
        <div class="panel-group" id="accordionTwo">
          <div class="panel panel-default">
            <div class="panel-heading">
              <h4 class="panel-title">
                <a class="accordion-toggle text-primary" data-toggle="collapse" data-parent="#accordionTwo" href="#collapseTwo">
                  <center><span class="h5 text-primary"><strong>Closing Rates for each Sales Coordinator</strong></span>
                  </center>
                </a>
              </h4>
            </div>
            <div id="collapseTwo" class="panel-collapse collapse">
              <div class="panel-body">
                <div id="closing-rate-individual">

                  <table class="table table-hover table-striped text-center table-bordered">
                    <thead>
                      <tr>
                        <th style="width:25%" class="text-center">User</th>
                        <th colspan="3" style="width:25%" class="text-center">First Data Set</th>
                        <th colspan="3" style="width:25%" class="text-center">Second Data Set</th>
                        <th colspan="3" style="width:25%" class="text-center">Difference</th>
                      </tr>
                      <tr>
                        <th style="width:25%" class="text-center"></th>
                        <th style="width:8.3%" class="text-center">Surgeries</th>
                        <th style="width:8.3%" class="text-center">Consults</th>
                        <th style="width:8.3%" class="text-center">Rate</th>
                        <th style="width:8.3%" class="text-center">Surgeries</th>
                        <th style="width:8.3%" class="text-center">Consults</th>
                        <th style="width:8.3%" class="text-center">Rate</th>
                        <th style="width:8.3%" class="text-center">Surgeries</th>
                        <th style="width:8.3%" class="text-center">Consults</th>
                        <th style="width:8.3%" class="text-center">Rate</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% @first_cr_users.each do |k, v| -%>
                      <tr>
                        <td><%= k.to_s.titleize %></td>
                        <!-- historic -->
                        <td><span><%= v[:surgeries] %></span></td>
                        <td><span><%= v[:consults] %></span></td>
                        <td><span><%= v[:rate].round(2) %>%</span></td>
                        <!-- comparitive -->
                        <td><span><%= @second_cr_users[k][:surgeries] %></span></td>
                        <td><span><%= @second_cr_users[k][:consults] %></span></td>
                        <td><span><%= @second_cr_users[k][:rate].round(2) %>%</span></td>
                        <!-- difference -->
                        <% surgery_difference = @second_cr_users[k][:surgeries] - v[:surgeries] %>
                        <% consult_difference =  @second_cr_users[k][:consults] - v[:consults] %>
                        <% rate_difference = ( @second_cr_users[k][:rate] - v[:rate] ).round(2) %>
                        <td class="<%= positive_or_negative?(surgery_difference)%>">
                          <span><%= surgery_difference %></span>
                        </td>
                        <td class="<%= positive_or_negative?(consult_difference)%>">
                          <span><%= consult_difference %></span>
                        </td>
                        <td class="<%= positive_or_negative?(rate_difference)%>">
                          <span><%= rate_difference %>%</span>
                        </td>
                      </tr>
                      <%- end -%>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div><!-- /panel -->
      </div>
    </div>
  </div><!-- /panel -->

  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading" style="border-bottom: solid red 1px;">
        <span class="h4">No Show and Appointment Cancel Rate</span>
        <ul class="tool-bar">
          <li><a href="#collapseFinancial" data-toggle="collapse"><i class="fa fa-arrows-v"></i></a></li>
        </ul>
      </div>
      <div class="panel-body" style="border-bottom: solid red 1px;">
        You can see the no show and cancel rate for the practice and how those affect the revenue.
      </div>
      <div class="panel-body no-padding collapse" id="collapseFinancial">
        <table class="table table-hover table-striped text-center table-bordered">
          <thead>
            <tr>
              <th class="text-center"><h5>Category</h5></th>
              <th class="text-center"><h5>First Data Set</h5></th>
              <th class="text-center"><h5>Second Data Set</h5></th>
              <th class="text-center"><h5>Difference</h5></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>No Show Rate</td>
              <td><%= @first_consult_count == 0 ? 0 : ((@first_no_show_consult_count.to_f / @first_consult_count) * 100).round(2)  %>%</td>
              <td><%= @second_consult_count == 0 ? 0 : ((@second_no_show_consult_count.to_f / @second_consult_count) * 100).round(2) %>%</td>
              <td class="<%=positive_or_negative?(((@second_consult_count == 0 ? 0 : ((@second_no_show_consult_count.to_f / @second_consult_count) * 100).round(2)) - (@first_consult_count == 0 ? 0 : ((@first_no_show_consult_count.to_f / @first_consult_count) * 100).round(2))).round(2))%>">
                <%= ((@second_consult_count == 0 ? 0 : ((@second_no_show_consult_count.to_f / @second_consult_count) * 100).round(2)) - (@first_consult_count == 0 ? 0 : ((@first_no_show_consult_count.to_f / @first_consult_count) * 100).round(2))).round(2)%>%
              </td>
            </tr>
            <tr>
              <td>Cancelled Consultations</td>
              <td><%= @first_consult_count == 0 ? 0 : ((@first_cancelled_consult_count.to_f / @first_consult_count) * 100).round(2)  %>%</td>
              <td><%= @second_consult_count == 0 ? 0 : ((@second_cancelled_consult_count.to_f / @second_consult_count) * 100).round(2) %></td>
              <td class="<%=positive_or_negative?(((@second_consult_count == 0 ? 0 : ((@second_cancelled_consult_count.to_f / @second_consult_count) * 100).round(2)) - (@first_consult_count == 0 ? 0 : ((@first_cancelled_consult_count.to_f / @first_consult_count) * 100).round(2))).round(2) )%>">
                <%= ((@second_consult_count == 0 ? 0 : ((@second_cancelled_consult_count.to_f / @second_consult_count) * 100).round(2)) - (@first_consult_count == 0 ? 0 : ((@first_cancelled_consult_count.to_f / @first_consult_count) * 100).round(2))).round(2) %>%
              </td>
            </tr>
            <tr>
